#include<iostream>
using namespace std;
int main()
{
    int arr[5];
    int n=5;
    for(int i=0;i<n;i++)
    {
        cout<<"enter the value of n:";
        cin>>arr[i];
    }
    int sum=0;
    for(int i=0;i<n;i++)
    {
        sum+=arr[i];
    }
    cout<<"total sum of:"<<sum<<" ";
    return 0;
}
